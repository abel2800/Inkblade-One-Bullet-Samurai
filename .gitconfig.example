# Git Configuration Example
# Copy this to your .gitconfig or run these commands

[user]
    name = Your Name
    email = your.email@example.com

[core]
    autocrlf = true
    editor = code --wait

[lfs]
    install = true

[push]
    default = simple

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = !gitk

